    "@mui/material": "^5.16.14",
    "@mui/x-date-pickers": "^7.27.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.67.3",
    "@tanstack/react-query-devtools": "^5.67.3",
    "@types/jspdf": "^2.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.29",
    "@types/xlsx": "^0.0.36",
    "antd": "^5.12.5",
    "axios": "^1.6.2",
